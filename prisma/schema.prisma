generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["views"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Formularios {
  id          Int       @id @default(autoincrement())
  tarefa      String?   @db.VarChar(255)
  horaInicial DateTime? @db.Time(0)
  horaFinal   DateTime? @db.Time(0)
  difHora     DateTime? @db.Time(0)
  peso        Float?    @db.Float
  material    String?   @db.VarChar(255)
  observacao  String?   @db.Text
  createdAt   DateTime  @db.DateTime(0)
  updatedAt   DateTime  @db.DateTime(0)
}

model Pontos {
  id           Int       @id @default(autoincrement())
  equipamentos String?   @db.VarChar(255)
  turnos       String?   @db.VarChar(255)
  contInicial  DateTime? @db.Time(0)
  contFinal    DateTime? @db.Time(0)
  createdAt    DateTime  @db.DateTime(0)
  updatedAt    DateTime  @db.DateTime(0)
}

model SequelizeMeta {
  name String @id @unique(map: "name") @db.VarChar(255)
}

