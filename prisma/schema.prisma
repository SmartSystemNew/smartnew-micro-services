model log_smartnewsystem_contrato {
  id                                                                 Int                            @id @default(autoincrement())
  id_cliente                                                         Int
  id_filial                                                          Int
  id_contrato                                                        Int
  id_aditivo                                                         Int?
  id_medicao                                                         Int?
  login_solicitante                                                  String                         @db.VarChar(50)
  login_aprovador                                                    String                         @db.VarChar(45)
  acao                                                               String                         @db.VarChar(45)
  log_date                                                           DateTime?                      @default(now()) @db.DateTime(0)
  cadastro_de_empresas                                               cadastro_de_empresas           @relation(fields: [id_cliente], references: [ID], onDelete: NoAction, onUpdate: NoAction, map: "key_id_cliente")
  smartnewsystem_contrato_numero                                     smartnewsystem_contrato_numero @relation(fields: [id_contrato], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "key_id_contrato")
  cadastro_de_filiais                                                cadastro_de_filiais            @relation(fields: [id_filial], references: [ID], onDelete: NoAction, onUpdate: NoAction, map: "key_id_filial")
  sec_users_log_smartnewsystem_contrato_login_aprovadorTosec_users   sec_users                      @relation("log_smartnewsystem_contrato_login_aprovadorTosec_users", fields: [login_aprovador], references: [login], onDelete: NoAction, onUpdate: NoAction, map: "key_login_aprovador")
  sec_users_log_smartnewsystem_contrato_login_solicitanteTosec_users sec_users                      @relation("log_smartnewsystem_contrato_login_solicitanteTosec_users", fields: [login_solicitante], references: [login], onDelete: NoAction, onUpdate: NoAction, map: "key_login_solicitante")

  @@index([id_cliente], map: "key_id_cliente_idx")
  @@index([id_contrato], map: "key_id_contrato_idx")
  @@index([id_filial], map: "key_id_filial_idx")
  @@index([login_aprovador], map: "key_login_aprovador_idx")
  @@index([login_solicitante], map: "key_login_solicitante_idx")
  @@map("_log_smartnewsystem_contrato")
}

